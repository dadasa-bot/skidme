local coregui = game:GetService("CoreGui")
local players = game:GetService("Players")

local localplayer = game.Players.LocalPlayer

local screenGui = Instance.new("ScreenGui", CoreGui)
screenGui.Name = "screenGui"

local btn = Instance.new("TextButton", screenGui)
btn.Name = "btn"
btn.Text = "Anti afk"
btn.Size = UDim2(0, 100, 0, 40)
btn.Position = UDim2(0, 30, 0, 30)
btn.BackgroundColor3 = Color3.fromRGB(40, 40, 120)
btn.TextColor3 = Color3.fromRGB(0, 0, 0)
btn.Font = Enum.Font.GothamBold
btn.TextScaled = true

local uibtn = Instance.new(UICorner", uibtn)
uibtn.CornerRadius = UDim.new(0, 10)

print("hi")

btn.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/anti-afk%20via%20autofocus.txt"))()
end
